<?xml version="1.0" encoding="UTF-8"?>
<E:mkcalendar xmlns:E="urn:ietf:params:xml:ns:caldav">
    <A:set xmlns:A="DAV:">
        <A:prop>
            <E:supported-calendar-component-set>
                <E:comp name="VTODO"/>
            </E:supported-calendar-component-set>
            <E:calendar-timezone>BEGIN:VCALENDAR&#13;
                VERSION:2.0&#13;
                CALSCALE:GREGORIAN&#13;
                BEGIN:VTIMEZONE&#13;
                TZID:Pacific/Auckland&#13;
                BEGIN:DAYLIGHT&#13;
                TZOFFSETFROM:+1200&#13;
                TZNAME:NZDT&#13;
                TZOFFSETTO:+1300&#13;
                RRULE:FREQ=YEARLY;BYMONTH=9;BYDAY=-1SU&#13;
                DTSTART:20070930T020000&#13;
                END:DAYLIGHT&#13;
                BEGIN:STANDARD&#13;
                TZOFFSETFROM:+1300&#13;
                TZNAME:NZST&#13;
                TZOFFSETTO:+1200&#13;
                RRULE:FREQ=YEARLY;BYMONTH=4;BYDAY=1SU&#13;
                DTSTART:20080406T030000&#13;
                END:STANDARD&#13;
                END:VTIMEZONE&#13;
                END:VCALENDAR&#13;
            </E:calendar-timezone>
            <I:calendar-order xmlns:I="http://apple.com/ns/ical/">1</I:calendar-order>
            <E:calendar-free-busy-set>
                <NO/>
            </E:calendar-free-busy-set>
            <A:displayname>Reminders</A:displayname>
        </A:prop>
    </A:set>
</E:mkcalendar>